{
  "source": {
    "include": "./lib"
  },
  "opts": {
    "destination": "docs",
    "pedantic": true,
    "recurse": true,
    "readme": "./README.md",
    "template": "node_modules/docdash"
  },
  "templates": {
    "default": {
      "includeDate": false
    },
    "docdash": {
      "sort": true
    }
  },
  "plugins": [
    "plugins/markdown",
    "plugins/summarize"
  ]
}
